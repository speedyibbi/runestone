{
  "name": "@runestone/server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsc -p tsconfig.json --watch\" \"nodemon dist/index.js\"",
    "build": "tsc -p tsconfig.json && npm run build:lambda",
    "build:lambda": "node dist/scripts/build-lambda.js",
    "start": "node dist/index.js",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@fastify/aws-lambda": "^4.0.0",
    "@runestone/config": "*",
    "fastify": "^5.6.0"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/node": "^22.16.5",
    "archiver": "^7.0.1",
    "concurrently": "^9.2.1",
    "esbuild": "^0.19.0",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "typescript": "~5.8.0"
  }
}
