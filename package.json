{
  "name": "runestone",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "services/*"
  ],
  "scripts": {
    "build": "npm run build --if-present --workspaces",
    "dev": "concurrently \"npm run dev -w @runestone/config\" \"npm run dev -w @runestone/web_app\"",
    "lint": "npm run lint --if-present --workspaces"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
